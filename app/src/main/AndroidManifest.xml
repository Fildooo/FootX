<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.footx">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/footxiconn"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/footxiconn"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpeedNewsApp">

        <activity
            android:name="com.example.footx.ChargementActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.footx.MainActivity"
            android:exported="true"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>